allow from all

RewriteEngine On

RewriteCond %{SERVER_PORT} !^8080$
RewriteRule ".?" "-" [S=2]

# if not port 8080 skip rewriting
    RewriteCond %{REQUEST_URI} ^/frontend/?$
    RewriteRule ^frontend/?$ /frontpage.html [R=302,S=3,L]
    # skip next two rules
    RewriteRule ^$ /frontpage.html [R=302,S=2,L]


RewriteCond %{SERVER_PORT} ^8090$
RewriteRule ".?" backend.timetracker.de:8090/%{REQUEST_URI} [R=302,S=1,L]

RewriteCond %{REQUEST_URI} -d
RewriteCond %{REQUEST_URI} -f
RewriteRule "(.*)" "notfound.php?file=$1&file2=$2&file3=$3"


